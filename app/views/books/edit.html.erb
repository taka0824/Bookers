<h1>Editing Book</h1>


<%= form_with model:@book,local:true do |f| %>

<%= render "layouts/error_messages", model: f.object %>

<h4>Title</h4>
<%= f.text_field :title %>

<h4>Body</h4>
<%= f.text_area :body %>

<%= f.submit "Update Book" %>

<% end %>

<%= link_to "Show",book_path(@book) %>
<%= link_to "Back",books_path %>